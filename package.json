{
  "dependencies": {
    "@biomejs/biome": "^2.2.0",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "bun-plugin-tailwind": "^0.0.15",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.539.0",
    "react": "^19",
    "react-dom": "^19",
    "react-router-dom": "^7.8.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.12",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "typescript": "~5.9.2"
  },
  "name": "recipe-organizer",
  "private": true,
  "scripts": {
    "build": "bun run build.ts && bun build ./dist/index.tsx --compile --minify --bytecode --outfile recipe_manager",
    "build-windows": "bun run build.ts && bun build ./dist/index.tsx --compile --minify --windows-hide-console --target=bun-windows-x64-modern --outfile recipe_manager.exe",
    "dev": "bun --hot src/index.tsx",
    "lint": "biome check",
    "lint-fix": "bun run lint -- --write",
    "ts-check": "tsc -p ."
  },
  "type": "module",
  "version": "0.1.0"
}
